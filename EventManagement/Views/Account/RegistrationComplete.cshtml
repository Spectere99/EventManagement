@model EventManagement.Models.RegisterViewModel
@{
    ViewBag.Title = "RegistrationComplete";
}

<h2>Registration Complete!</h2>

<h2>@ViewBag.Title.</h2>
<div class="col-md-6">
    <p>
        Thank you for registering! Please check your email at <strong>@ViewBag.registrationEmail</strong> for your confirmation code.  Click on the link contained in that email to confirm your email
        then you will be able to login to the site.
    </p>
</div>

