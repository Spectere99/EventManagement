@model EventManagement.Models.RegisterViewModel
@{
    ViewBag.Title = "RegistrationComplete";
}

<h2>@ViewBag.Title.</h2>


<div class="col-md-12">
    <p>
        Thank you for creating your account!
        You may now login to the site and register for the event.
        <a class="btn btn-default" href="../Registration/AttendeeSelection">Click Here to Register!</a>
        @*Please check your email at <strong>@ViewBag.registrationEmail</strong> for your confirmation code.  Click on the link contained in that email to confirm your email
        then you will be able to login to the site.*@
    </p>
</div>

